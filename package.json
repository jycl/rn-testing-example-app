{
  "name": "TestExampleApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-native": "0.56.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.2",
    "detox": "^9.0.1",
    "jest": "^23.6.0",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/TestExampleApp.app",
        "build": "xcodebuild -project ios/TestExampleApp.xcodeproj -scheme TestExampleApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "jest"
  }
}